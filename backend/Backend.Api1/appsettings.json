{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "Microsoft.Hosting.Lifetime": "Verbose",
        "Microsoft.AspNetCore.Authentication": "Verbose",
        "System": "Verbose"
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Certificates": {
      "Default": {
        "Path": "../../certs/localhost/localhost.pfx",
        "Password": "123456789",
        "AllowInvalid": true
      }
    }
  }
}
